<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>StorageBins}"
		items="{
			path: '/ZSCM_I_StorageBinVH'
		}"
		search=".onValueHelpSearch($event,'DSB')"
		confirm=".onValueHelpConfirm($event,'DSB')"
		cancel=".onValueHelpClose">
		<StandardListItem
			title="{StorageBin}" />
	</SelectDialog>
</core:FragmentDefinition>