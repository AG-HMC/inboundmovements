<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>StorageTypes}"
		items="{
			path: '/ZSCM_I_StorageTypeVH'
		}"
		search=".onValueHelpSearch($event,'DST')"
		confirm=".onValueHelpConfirm($event,'DST')"
		cancel=".onValueHelpClose">
		<StandardListItem
			title="{StorageType}"
			description="{StorageType_Text}"  />
	</SelectDialog>
</core:FragmentDefinition>