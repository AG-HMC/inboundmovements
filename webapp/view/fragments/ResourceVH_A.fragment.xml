<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		title="{i18n>Resources}"
		items="{
			path: '/FetchExecutiveResourceList'
		}"
		search=".onValueHelpSearch($event,'RESOURCE')"
		confirm=".onValueHelpConfirm($event,'RESOURCE_A')"
		cancel=".onValueHelpClose">
		<StandardListItem
			title="{ExecutingResource}"/>
	</SelectDialog>
</core:FragmentDefinition>